{
  "name": "wcmv3demo-app",
  "version": "0.0.0",
  "description": "Wcmv3demo app",
  "keywords": [
    "angular",
    "typescript"
  ],
  "author": "",
  "homepage": "",
  "config": {
    "app": {
      "port": 3000
    }
  },
  "scripts": {
    "start": "npm run server",
    "postinstall": "npm run build:vendor",
    "********************** CLEAR **********************": "",
    "clean": "rimraf \"node_modules/!(rimraf|.bin\" doc dist && npm cache clean",
    "********************** LINTING **********************": "",
    "lint": "tslint --force \"src/**/*.ts\"",
    "********************** TEST **********************": "",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,karma.entry.ts",
    "coverage:report": "istanbul report",
    "coverage:serve": "cross-env NODE_ENV=TEST lite-server --baseDir=\"./coverage/lcov-report\"",
    "test": "cross-env NODE_ENV=TEST karma start",
    "test:watch": "npm run test -- --no-single-run --auto-watch",
    "posttest": "npm run coverage",
    "********************** BUILD **********************": "",
    "build": "npm-run-all -s build:vendor build:dist",
    "build:dist": "rimraf dist && cross-env NODE_ENV=PROD webpack --progress --profile --bail",
    "build:vendor": "webpack --config ./config/webpack.config.vendor",
    "********************** TRANSLATIONS **********************": "",
    "extract": "ng2-translate-extract --dir ./src --output ./src/public/translations --format=namespaced-json --clean --sort",
    "********************** SERVE **********************": "",
    "lite-server": "lite-server",
    "server": "cross-env NODE_ENV=DEV webpack-dev-server --inline --progress --port $npm_package_config_app_port --open",
    "server:hmr": "npm run server -- --hot",
    "server:prod": "npm-run-all -s build lite-server",
    "server:aot": "cross-env AOT=true npm run server",
    "server:prod:aot": "cross-env AOT=true npm run server:prod",
    "********************** SELENIUM **********************": "",
    "webdriver:start": "webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "dependencies": {
    "@acpaas-ui/aui-kit": "^1.1.0",
    "@acpaas-ui/footer": "^2.0.3",
    "@acpaas-ui/header": "^1.0.7",
    "@acpaas-ui/logo": "^1.0.3",
    "@angular-redux/store": "~6.2.1",
    "@angular/animations": "4.1.0",
    "@angular/common": "~4.1.0",
    "@angular/compiler": "~4.1.0",
    "@angular/compiler-cli": "~4.1.0",
    "@angular/core": "~4.1.0",
    "@angular/forms": "~4.1.0",
    "@angular/http": "~4.1.0",
    "@angular/platform-browser": "~4.1.0",
    "@angular/platform-browser-dynamic": "~4.1.0",
    "@angular/platform-server": "~4.1.0",
    "@angular/router": "~4.1.0",
    "@ngx-translate/core": "~6.0.0",
    "@ngx-translate/http-loader": "0.0.3",
    "core-js": "~2.4.1",
    "express": "^4.15.2",
    "glob": "^7.1.1",
    "http-proxy": "^1.16.2",
    "immutable": "~3.8.1",
    "lodash": "^4.17.4",
    "redux": "~3.6.0",
    "redux-thunk": "~2.2.0",
    "reflect-metadata": "~0.1.3",
    "rootpath": "^0.1.2",
    "rxjs": "5.3.1",
    "typed-immutable-record": "0.0.6",
    "wcm-template-manager-ng2": "git+ssh://git@github.com/hvperdrive/wcm-template-manager-ng2.git#v3.0.0",
    "zone.js": "~0.8.9"
  },
  "devDependencies": {
    "@biesbjerg/ng2-translate-extract": "~0.6.0",
    "@ngtools/webpack": "~1.3.1",
    "@types/core-js": "0.9.35",
    "@types/jasmine": "~2.5.47",
    "@types/node": "~6.0.38",
    "@types/selenium-webdriver": "3.0.3",
    "angular2-template-loader": "~0.6.0",
    "autoprefixer": "~6.7.7",
    "awesome-typescript-loader": "~3.1.3",
    "chalk": "~1.1.3",
    "codelyzer": "~3.0.1",
    "copy-webpack-plugin": "~4.0.0",
    "cross-env": "~4.0.0",
    "extract-text-webpack-plugin": "~2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "~0.11.1",
    "html-loader": "~0.4.0",
    "html-webpack-plugin": "~2.28.0",
    "istanbul-instrumenter-loader": "~2.0.0",
    "jasmine-core": "~2.6.1",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.6.0",
    "karma-chrome-launcher": "~2.1.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-mocha-reporter": "~2.2.3",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.2",
    "lite-server": "~2.3.0",
    "npm-run-all": "~4.0.2",
    "node-sass": "~4.5.2",
    "postcss-loader": "~1.3.3",
    "protractor": "~5.1.1",
    "raw-loader": "0.5.1",
    "remap-istanbul": "~0.9.1",
    "rimraf": "~2.6.1",
    "sass-loader": "~6.0.3",
    "style-loader": "~0.17.0",
    "stylelint-config-standard": "~16.0.0",
    "stylelint-webpack-plugin": "~0.7.0",
    "tslib": "~1.6.0",
    "tslint": "~5.1.0",
    "tslint-loader": "~3.5.3",
    "typescript": "~2.3.0",
    "webpack": "~2.4.1",
    "webpack-dev-server": "~2.4.5"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
